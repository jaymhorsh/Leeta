{
  "expo": {
    "name": "Leeta App",
    "slug": "leeta-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "runtimeVersion": "1.0.0",
    "icon": "./assets/images/adaptive-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "scheme": "leeta",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jaymhorsh.leeta"
    },
    "android": {
      "package": "com.jaymhorsh.leeta",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.ACCESS_COARSE_LOCATION", "android.permission.ACCESS_FINE_LOCATION", "android.permission.CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "buildReactNativeFromSource": true
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Leeta needs camera access to capture delivery confirmation photos.",
          "photosPermission": "Leeta needs access to your photos for profile or address verification."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Leeta to use your location to deliver gas to your location."
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9172b9e5-ef3e-4f83-9331-f44a1cea9dea"
      }
    },
    "owner": "jaymhorsh",
    "updates": {
      "url": "https://u.expo.dev/9172b9e5-ef3e-4f83-9331-f44a1cea9dea"
    }
  }
}
